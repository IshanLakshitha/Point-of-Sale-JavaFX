<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="478.0" prefWidth="544.0" style="-fx-background-color: #3C3C3C;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.fx.controllers.SearchOderController">
   <children>
       <TextField fx:id="txtSearch" layoutX="146.0" layoutY="82.0" onKeyReleased="#search" prefHeight="26.0" prefWidth="333.0" promptText="Enter Oder ID" />
      <Label layoutX="78.0" layoutY="87.0" text="Search :" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Button fx:id="btnBack" layoutX="463.0" layoutY="13.0" mnemonicParsing="false" onAction="#clickBack" prefHeight="25.0" prefWidth="66.0" text="Back" />
      <TableView fx:id="tblOrderDetails" layoutX="14.0" layoutY="145.0" onMouseClicked="#ClickRow" prefHeight="307.0" prefWidth="515.0">
        <columns>
          <TableColumn prefWidth="82.0" text="Order ID" />
          <TableColumn prefWidth="203.0" text="Customer ID" />
            <TableColumn prefWidth="117.0" text="Order Date" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnReport" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#clickReport" text="Report" />
   </children>
</AnchorPane>
